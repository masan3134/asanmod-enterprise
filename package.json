{
  "name": "[PROJECT_NAME]",
  "version": "3.2.0",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/check-first-run.js",
    "prepare": "husky install || true",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix": "next lint --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "node --env-file=.env node_modules/.bin/drizzle-kit push:pg",
    "db:bootstrap": "node scripts/db-bootstrap.js",
    "wizard": "node scripts/mod-tools/asan-wizard.js",
    "init": "node scripts/mod-tools/asan-init.js",
    "deploy-prod": "bash scripts/mod-tools/deploy-prod.sh",
    "seed": "tsx --env-file=.env scripts/seed.ts",
    "db:seed:dev": "tsx --env-file=.env scripts/seed-dev.ts",
    "db:reset": "npm run db:push && npm run db:seed:dev",
    "create-admin": "tsx --env-file=.env scripts/create-admin.ts",
    "backup": "bash scripts/backup-db.sh",
    "restore": "bash scripts/restore-db.sh",
    "reset-dev": "bash scripts/reset-dev.sh",
    "health": "bash scripts/mod-tools/health-check.sh",
    "verify": "bash scripts/mod-tools/verify.sh",
    "doctor": "node scripts/mod-tools/doctor.js",
    "bump": "node scripts/mod-tools/version-manager.cjs bump",
    "version:sync": "node scripts/mod-tools/version-manager.cjs sync",
    "db:health": "node scripts/mod-tools/db-health-check.cjs",
    "db:health:dev": "node scripts/mod-tools/db-health-check.cjs dev",
    "db:health:prod": "node scripts/mod-tools/db-health-check.cjs prod",
    "db:sync-check": "node scripts/mod-tools/db-sync-check.cjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tanstack/react-query": "^5.25.0",
    "@trpc/client": "^11.0.0-rc.300",
    "@trpc/next": "^11.0.0-rc.300",
    "@trpc/react-query": "^11.0.0-rc.300",
    "@trpc/server": "^11.0.0-rc.300",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "next": "^14.2.0",
    "pg": "^8.16.3",
    "pg-mem": "^3.0.5",
    "postgres": "^3.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.49.3",
    "superjson": "1.13.3",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.17",
    "drizzle-kit": "^0.20.14",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masan3134/asanmod-enterprise.git"
